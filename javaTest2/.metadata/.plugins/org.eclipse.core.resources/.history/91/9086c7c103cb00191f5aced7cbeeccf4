package chapter07.ex10;

public class Triangle extends Shape {
	private int length;
	private int area;
	
	public int getLength() {
		return this.length=super.getHeight()+super.getWidth();
	}
	public int getArea() {
		return this.area=super.getHeight()*super.getWidth();
		
	}
	@Override
	public String toString() {
		return "Triangle [length=" + length + ", area=" + area + "]";
	}
	

}
